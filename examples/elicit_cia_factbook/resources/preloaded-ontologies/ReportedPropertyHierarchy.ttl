    owl:imports <http://ontology.causeex.com/ontology/odps/GeneralConcepts> ;
    owl:imports <http://ontology.causeex.com/ontology/odps/TimeSeriesAndMeasurements> ;
    owl:versionInfo "Version 0.5"^^xsd:string .

